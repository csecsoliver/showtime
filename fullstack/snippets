await replaceUserSession(event, {
  user: {
    name: body.username,
  },
  secure: {
    apiToken: token("gen", body.username),
  },
});

import { createStorage } from "unstorage";
import fsDriver from "unstorage/drivers/fs";
const any = createStorage({
  driver: fsDriver({base: "./any"})
})

